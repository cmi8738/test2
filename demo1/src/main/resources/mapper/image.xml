<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zms.demo1.CDao.CimageDao">
    <select id="findAllCimage" resultType="com.zms.demo1.entiy.Cimage">
        select * from cimage
    </select>
    <insert id="addCimage" parameterType="com.zms.demo1.entiy.Cimage">
        insert into cimage(cimid,imcite)VALUES (#{cimid},#{imcite})
    </insert>
</mapper>